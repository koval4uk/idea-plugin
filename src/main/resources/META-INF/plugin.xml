<idea-plugin>
    <id>org.code.wars</id>
    <name>Codewars</name>
    <vendor email="akovalchuk@gmail.comm" url="http://github.com/koval4uk">Artur Kovalchuk</vendor>

    <description>My first idea plugin. My first idea plugin. My first idea plugin.</description>

    <!-- please see https://www.jetbrains.org/intellij/sdk/docs/basics/getting_started/plugin_compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <!-- Add your extensions here -->
        <httpRequestHandler implementation="org.code.wars.ApplyPatchService"/>
    </extensions>

    <actions>
        <!-- Add your actions here -->
        <action id="MyIdea.Demo" class="org.code.wars.MyDemoAction"/>
<!--        <action id="MyIdea.Demo" class="com.intellij.openapi.vcs.changes.patch.ApplyPatchFromClipboardAction"/>-->
    </actions>

</idea-plugin>